            type="console.py" 
            respawn="false" 
            output="screen">
            <!-- custom 1 -->
            <param name="px1" type="double" value="0.3"/>
            <param name="py1" type="double" value="0.0"/>
            <param name="pz1" type="double" value="0.0"/>

            <param name="ox1" type="double" value="0.0"/>
